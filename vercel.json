{
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Content-Security-Policy",
                    "value": "frame-ancestors 'self' https://telegram.org https://*.telegram.org https://telegram-web-app.github.io"
                },
                {
                    "key": "X-Frame-Options",
                    "value": "ALLOW-FROM https://telegram.org/ https://web.telegram.org/"
                }
            ]
        }
    ]
}